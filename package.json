{
  "name": "employee-management-api",
  "version": "1.0.0",
  "description": "RESTful API for employee management system with TypeScript, Express, and PostgreSQL",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:backend": "./tests/backend-validation.sh",
    "validate": "./tests/backend-validation.sh",
    "test:e2e": "npx playwright test",
    "test:e2e:headed": "npx playwright test --headed",
    "test:e2e:debug": "npx playwright test --debug",
    "test:e2e:report": "npx playwright show-report",
    "test:prd": "npx playwright test --project='PRD Critical'",
    "test:ai": "npx playwright test ai-forecasting.spec.ts skill-matching.spec.ts optimization.spec.ts",
    "test:accessibility": "npx playwright test --project='Accessibility'",
    "test:performance": "npx playwright test --project='Performance'",
    "test:mobile": "npx playwright test --project='Mobile Chrome'",
    "test:all": "npm run test && npm run test:e2e",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "db:migrate": "ts-node migrations/migrate.ts",
    "db:seed": "ts-node seeds/seed.ts",
    "docker:build": "docker build -t employee-api .",
    "docker:run": "docker run -p 3001:3001 employee-api"
  },
  "keywords": [
    "employee",
    "management",
    "api",
    "rest",
    "typescript",
    "express",
    "postgresql",
    "crud"
  ],
  "author": "Backend API Developer",
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "@types/bull": "^3.15.9",
    "@types/handlebars": "^4.0.40",
    "@types/ioredis": "^4.28.10",
    "@types/lodash": "^4.17.20",
    "@types/mathjs": "^9.4.1",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.1",
    "@types/reflect-metadata": "^0.0.5",
    "@types/sequelize": "^4.28.20",
    "@types/socket.io": "^3.0.1",
    "@types/socket.io-client": "^1.4.36",
    "@types/web-push": "^3.6.4",
